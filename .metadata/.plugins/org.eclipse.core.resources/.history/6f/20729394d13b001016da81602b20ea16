package com;

public class WaitNotify {

	public static void main(String[] args) {
		
		Thread5 t5 = new Thread5();
		t5.start();
		System.out.println(t5.count);

	}

}

class Thread5 extends Thread{
	
	int count =0;
	@Override
	public void run() {
		
		for(int i =1; i<=100; i++) {
			count++;
		}
	}
	
}
